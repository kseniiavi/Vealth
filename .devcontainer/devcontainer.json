{
    "name": "Flutter Web Codespace",
    "image": "ghcr.io/cirruslabs/flutter:stable",
    "forwardPorts": [3000, 8080],
    "portsAttributes": {
      "3000": {
        "label": "Flutter Web App",
        "onAutoForward": "openBrowser"
      }
    },
    "customizations": {
      "vscode": {
        "extensions": [
          "dart-code.flutter",
          "dart-code.dart-code",
          "ms-vscode.vscode-json"
        ]
      }
    },
    "postCreateCommand": "flutter doctor && flutter config --enable-web",
    "remoteUser": "root"
  }
  